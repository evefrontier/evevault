{
  "name": "eve-frontier-vault",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "services"
  ],
  "packageManager": "bun@1.3.1",
  "scripts": {
    "dev": "turbo run dev",
    "dev:ext": "turbo run dev --filter=@evevault/extension",
    "dev:ext:direct": "bun --cwd apps/extension run dev",
    "dev:web": "turbo run dev --filter=@evevault/web",
    "build": "turbo run build",
    "build:ext": "turbo run build --filter=@evevault/extension",
    "build:web": "turbo run build --filter=@evevault/web",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "clean:cache": "node scripts/clean-cache.js",
    "clean:all": "./scripts/clean-all.sh",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:ui": "turbo run test:ui",
    "test:run": "turbo run test:run",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "dependencies": {
    "@mysten/sui": "^1.43.1",
    "@mysten/wallet-standard": "^0.16.3",
    "jose": "6.0.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.14",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "turbo": "^2.6.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14"
  }
}
